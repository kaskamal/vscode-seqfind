{
    "name": "vscode-seqfind",
    "displayName": "vscode-SeqFind",
    "description": "NULL",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:seqfind.search"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "seqfind.search",
            "title": "SeqFind Search"
        }],
        "keybindings": [{
            "command": "seqfind.search",
            "key": "alt+s",
            "when": ""
        }],
        "colors": [{
            "id": "seqfind.identicalBorder",
            "description": "Border color for identical sequence matches",
            "defaults": {
                "dark": "#00f81555",
                "light": "#00f81555",
                "highContrast": "#00f81555"
            }
        }, {
            "id": "seqfind.complementBorder",
            "description": "Border color for complement sequence matches",
            "defaults": {
                "dark": "#009df855",
                "light": "#009df855",
                "highContrast": "#009df855"
            }
        }, {
            "id": "seqfind.reverseComplementBorder",
            "description": "Border color for reverse complement sequence matches",
            "defaults": {
                "dark": "#f8250055",
                "light": "#f8250055",
                "highContrast": "#f8250055"
            }
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "tslint": "^5.12.1",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}